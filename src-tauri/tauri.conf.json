{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "sysproxy-tauri-demo",
  "version": "0.1.0",
  "identifier": "com.example.sysproxydemo",
  "build": {
    "frontendDist": "../src"
  },
  "bundle": {
    "externalBin": [
      "sidecar/verge-mihomo"
    ],
    "icon": [
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ]
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "capabilities": [
        {
          "identifier": "main-capability",
          "description": "Main window capabilities",
          "windows": [
            "main"
          ],
          "permissions": [
            "core:default",
            "core:event:allow-listen",
            "core:event:allow-emit"
          ]
        }
      ]
    },
    "windows": [
      {
        "label": "main",
        "title": "Sysproxy Demo",
        "width": 800,
        "height": 1000,
        "resizable": true
      }
    ]
  }
}